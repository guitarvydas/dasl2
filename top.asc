asc [top](NIL (atom space) (cons space) src out) > (out)
    include eval.type.dasl
    include eval.decl.dasl
    $wait
    $return out

    components:
        [read-eval-format](src)(out) > (out)
    end components

    connections    
        [read-eval-format](out) -> [$self](out)
    end connections    
end [top]