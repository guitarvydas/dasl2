in top.asc
asc [read] >> (index (atom space modified))
    import NIL (atom space) (cons space) (src)
    $unless ?(lookahead lpar) $then
        $inject $trigger >> [read atom](start)
    $else
        $inject $trigger >> [read list](start)
    $end if
    $wait
    $return (index (atom space))

    components
	[read atom](start) >> (answer)
	[read list](start) >> (answer)
    end components

    connections
        [read atom](answer) >> [$self](index)
        [read list](answer) >> [$self](index)
    end connections

end [read]