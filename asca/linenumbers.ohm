basictoken {
// basic token
  Main = Token+
  Token = "{" dq "token" dq ":" string "," dq "content" dq ":" string "}" ","?
  string = dq stringChar* dq
  dq = "\""
  stringChar = ~dq any


}

linenumbers <: basictoken {

  Main := LineNumberingToken+

  LineNumberingToken = NLToken | Token
  NLToken = "{" dq "token" dq ":" dq "nl" dq "," dq "content" dq ":" string "}" ","?

}
